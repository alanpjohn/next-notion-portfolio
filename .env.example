# Google Analytics
NEXT_PUBLIC_GOOGLE_ANALYTICS=<Your_Google_Analytics_Tracking_ID>
ANALYTICS_ENABLED=true

# Redis Configuration (for preview image caching)
REDIS_HOST=<REDIS_HOST>
REDIS_PASSWORD=<REDIS_PASSWORD>
REDIS_USER=default
REDIS_URL=<REDIS_URL_OR_LEAVE_EMPTY_TO_USE_HOST_PASSWORD>
REDIS_NAMESPACE=preview-images

# Domain Configuration
DOMAIN=<YOUR_DOMAIN_OR_LEAVE_EMPTY_FOR_DEFAULT>

# Development/Build Configuration
NODE_ENV=development
PORT=3000
ANALYZE=false
